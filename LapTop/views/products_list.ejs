    <div class="data-table-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="data-table-list">
                        <div class="basic-tb-hd">
                            <h2>Danh sách sản phẩm </h2>
                            <p>Cập nhập mới nhất</p>
                        </div>
                        <div class="table-responsive">
                            <table id="data-table-basic" class="table table-striped">
                                <thead>

                                    <tr>
                                        <th>idproduct</th>
                                        <th>nameproduct</th>
                                        <th>priceEnter</th>
                                        <th>saleprice</th>
                                        <th>amount</th>
                                        <th>dateadd</th>
                                        <th>describe</th>
                                        <th>idimage</th>
                                        <th>image</th>
                                        <th>idbrand</th>
                                        <th>namebrand</th>
                                        <th>Fix</th>
                                        <th>Delete</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <% sps.forEach(function(sp){ %>
                                    <tr>
                                        <td><%= sp.idProduct %></td>
                                        <td><%= sp.nameProduct %></td>
                                        <td><%= sp.priceEnter %></td>
                                        <td><%= sp.salePrice %></td>
                                        <td><%= sp.amount %></td>
                                        <td><%= sp.dateAdd %></td>
                                        <td><%= sp.describe %></td>
                                        <td><%= sp.idImage %></td>
                                        <td><img width="160px" src="../images/LapTop/<%= sp.image %>" alt="" /></td>
                                        <td><%= sp.idBrand %></td>
                                        <td><%= sp.nameBrand %></td>
                                        <th>
                                            <div class="card-footer">
                                                <button type="submit" class="btn btn-success btn-sm" name="btnSuaSP"
                                                    value="SuaSP" />
                                                <a href="http://localhost:3001/suaSP/<%= sp.idproduct %>"
                                                    style="color:white" />
                                                <i class="fa fa-save"></i> Edit
                                                </a>
                                                </button>
                                            </div>
                                        <th>
                                            <div class="card-footer">
                                                <button type="submit" class="btn btn-danger btn-sm" name="btnXoaSP"
                                                    value="XoaSP" />
                                                <a onclick="return confirm('Are you sure to delete')"
                                                    href="http://localhost:3001/xoaSP/<%= sp.idproduct %>"
                                                    style="color:white" />
                                                <i class="fa fa-remove"></i> Delete
                                                </a>
                                                </button>
                                            </div>
                                        </th>
                                    </tr>
                                    <%});%>


                                </tbody>

                            </table>
                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-sm" name="btnThemSP" value="ThemSP" />
                                <a href="http://localhost:3001/themSP" style="color:white" />
                                <i class="fa fa-subway"></i> AddProduct
                                </a>
                                </button>
                            </div>


                        </div>
                    </div>
                </div>
